@{
    ViewData["Title"] = "Technical Indicators MACD Result";
}

<h2>Technical Indicators Result</h2>

<div>
    <p><strong>Short Moving Average (ma1):</strong> @ViewData["Arg0"]</p>
    <p><strong>Long Moving Average (ma2):</strong> @ViewData["Arg1"]</p>
    <p><strong>Start Date:</strong> @ViewData["Arg2"]</p>
    <p><strong>End Date:</strong> @ViewData["Arg3"]</p>
    <p><strong>Stock Ticker:</strong> @ViewData["Arg4"]</p>
    <p><strong>Data Slicer:</strong> @ViewData["Arg5"]</p>
</div>

<h3>Python Script Output</h3>
<pre>@ViewBag.PythonOutput</pre>

@* <h3>Python Script Error</h3>
<pre>@ViewBag.PythonError</pre> *@

<h3>Generated Plots</h3>
<div>
    <img src="~/images/output_positions_MACD.png" alt="Positions Plot" />
    <img src="~/images/output_macd.png" alt="MACD Plot" />

    @* <img src="@ViewBag.positionsPathMACD" alt="Positions Plot" />
    <img src="@ViewBag.MacdPath" alt="MACD Plot" /> *@
</div>
